 <h1>商品一覧</h1>
 <% @products.each do |f| %>
 	<div>
 	<%= link_to product_path(f.id) do %>
 	  <%= attachment_image_tag @product, :product_image, :fill, 300, 300, fallback:"No-Image-Available.png",size: :"300x300" %>
      <% end %>
      <span><%= f.product_name %></span>
        <span><%= f.non_tax %></span>
 	</div>
 	<% end %>